{
  "name": "@larsv94/react-ts-plop",
  "version": "0.2.1",
  "description": "Plop-pack for React Typescript components",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf ./lib && tsc --emitDeclarationOnly --out lib/index.js && parcel build ./src/index.ts --target node -d lib --public-url ./",
    "dev": "parcel ./src/index.ts --target node -d lib",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "plop",
    "react",
    "components",
    "generator"
  ],
  "author": "Larsv94",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2"
  },
  "files": [
    "lib/**/*",
    "templates/**/*"
  ],
  "dependencies": {
    "change-case": "^4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Larsv94/react-ts-plop.git"
  }
}